             </ImageView>
             <ImageView fx:id="doublePointsJoker" fitHeight="112.0" fitWidth="70.0" layoutX="88.0" layoutY="7.0" onMouseClicked="#changeJoker2" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
