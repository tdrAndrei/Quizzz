             <ImageView fx:id="eliminateJoker" fitHeight="112.0" fitWidth="70.0" layoutX="12.0" layoutY="7.0" onMouseClicked="#changeJoker1" pickOnBounds="true" preserveRatio="true" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
