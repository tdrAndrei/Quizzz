<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="282.0" prefWidth="506.0" stylesheets="@/client.css/application.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.LoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="267.0" prefWidth="506.0" style="-fx-background-color: #fff4d1;" VBox.vgrow="ALWAYS">
      <children>
            <VBox layoutX="81.0" layoutY="143.0" prefHeight="200.0" prefWidth="390.0">
               <children>
                  <TextField fx:id="nameBox" onAction="#submit" prefHeight="26.0" prefWidth="304.0">
                     <tooltip>
                        <Tooltip text="This is how other player will see you." />
                     </tooltip>
                  </TextField>
                  <TextField fx:id="urlBox" text="http://localhost:8080/">
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </TextField>
                  <ButtonBar prefHeight="40.0" prefWidth="200.0">
                    <buttons>
                      <Button fx:id="submitButton" mnemonicParsing="false" onAction="#submit" text="Submit" />
                        <Button fx:id="quitButton" mnemonicParsing="false" onAction="#quit" text="Quit" />
                    </buttons>
                     <VBox.margin>
                        <Insets right="130.0" top="30.0" />
                     </VBox.margin>
                  </ButtonBar>
               </children>
            </VBox>
            <Group layoutX="39.0" layoutY="156.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name" />
                  <Text layoutX="5.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="URL" />
               </children>
            </Group>
            <Label fx:id="warnBox" alignment="CENTER" layoutX="56.0" layoutY="120.0" prefHeight="18.0" prefWidth="224.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>
